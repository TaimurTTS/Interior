body {
  font-family: 'montserratlight', Helvetica, Arial, sans-serif;
  font-size: 14px;
  line-height: 1.42857143;
  color: #333;
  background-color: #fff;
}


.krank-main-nav ul{}
.krank-main-nav li {
    font-family: "montserratlight";
    font-size: 14px;
}
.krank-main-nav li a{color:#FFF;}

.banner-img {
/*background-image: url("../img/banner.jpg");*/
    background-position: 100% center;
    background-repeat: no-repeat;
    background-size: cover;
    text-align: center;
    width: 100%;
}
.banner-img h1 {
    color: #FFF;
    font-size: 39px;
    font-family: "montserratlight";
    margin-bottom: 30px;
}
.mar-top-1{ margin-bottom: 14%;
    margin-top: 31%;}
	
	.banner-img a.btn-signup {
    background-color: #1b75bc;
    border-radius: 2px;
    color: #ffffff;
    font-family: "montserratlight";
    font-size: 15px;
    padding: 10px 20px;
}
.client-logo{background-color: #f4f4f4; width:100%;}

